<ComponentSchema packageName="com.iwsdk.components">
  <Enum name="PhysicsState">
    <EnumValue value="Static"/>
    <EnumValue value="Dynamic"/>
  </Enum>
  <Component name="com.elics.PhysicsBody" description="EliCS component: PhysicsBody">
    <FloatAttribute name="linearVelocity" defaultValue="0f"/>
    <FloatAttribute name="angularVelocity" defaultValue="0f"/>
    <Vector3Attribute name="applyForce" defaultValue="0f, 0f, 0f"/>
    <EnumAttribute name="state" defaultValue="PhysicsState.Dynamic"/>
    <FloatAttribute name="density" defaultValue="1f"/>
    <FloatAttribute name="restitution" defaultValue="0.5f"/>
    <FloatAttribute name="friction" defaultValue="0.5f"/>
  </Component>
</ComponentSchema>